{
  "name": "asktris",
  "version": "1.0.0",
  "description": "fullstack asktris project",
  "main": "index.js",
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.22.5",
    "@babel/core": "7.21.8",
    "@babel/node": "7.22.5",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-export-default-from": "7.22.5",
    "@babel/plugin-proposal-export-namespace-from": "7.18.9",
    "@babel/plugin-transform-flow-strip-types": "7.22.5",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.22.5",
    "@babel/preset-flow": "7.22.5",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@babel/register": "7.22.5",
    "@types/babel__core": "7.20.0",
    "@types/babel__generator": "7.6.4",
    "@types/babel__template": "7.4.1",
    "@types/babel__traverse": "7.20.1",
    "@types/bcryptjs": "^2.4.1",
    "@types/dotenv-safe": "^8.1.1",
    "@types/graphql": "^14.2.2",
    "@types/graphql-relay": "^0.7.0",
    "@types/koa": "^2.13.6",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/koa-convert": "^1.2.0",
    "@types/koa-cors": "^0.0.2",
    "@types/koa-logger": "^3.1.0",
    "@types/koa-multer": "^1.0.0",
    "@types/koa-router": "^7.0.37",
    "@types/mongoose": "^5.3.7",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-watch": "^7.4.1",
    "eslint": "6.8.0",
    "turbo": "1.10.5",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-okonet": "7.0.2",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-shellscape": "6.0.1",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-cypress": "2.13.3",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-relay": "1.8.3",
    "eslint-plugin-typescript": "0.14.0",
    "jest": "26.6.3",
    "jest-junit": "16.0.0",
    "jsonwebtoken": "^9.0.0",
    "lint-staged": "^13.2.2",
    "node-dev": "^8.0.0",
    "nodemon": "2.0.22",
    "pre-commit": "^1.2.2",
    "prettier": "2.8.8",
    "typescript": "5.1.3"
  },
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn prettier",
      "eslint --fix",
      "git add"
    ],
    "*.yml": [
      "prettier --write",
      "git add"
    ]
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "apps/*"
    ]
  },
  "scripts": {
    "clean": "turbo run clean && rm -rf node_modules",
    "changeset-create": "changeset",
    "changeset-apply": "changeset version",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx,md}\"",
    "build": "turbo run build",
    "build-all": "yarn build:modules && yarn build:shared && yarn build:types && yarn build:test",
    "build:shared": "yarn workspace @asktris/shared build",
    "build:types": "yarn workspace @asktris/types build",
    "build:test": "yarn workspace @asktris/test build",
    "release": "turbo run build && changeset publish",
    "storybook": "yarn workspace @asktris/web storybook",
    "jest": "jest",
    "jest:server": "yarn workspace @asktris/server jest",
    "jest:web": "yarn workspace @asktris/web jest",
    "lint": "turbo run lint",
    "lint-old": "eslint .",
    "lint:staged": "lint-staged",
    "prettier": "prettier --write --single-quote true --trailing-comma all --print-width 120",
    "server": "yarn workspace @asktris/server start",
    "web": "yarn workspace @asktris/web dev"
  },
  "dependencies": {
    "@types/supertest": "^2.0.12",
    "axios": "^1.4.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "jest-environment-node": "^26.0.1",
    "jest-fetch-mock": "^3.0.3",
    "koa-cors": "^0.0.16",
    "koa-logger": "^3.2.1",
    "moment": "^2.29.4",
    "mongodb-memory-server": "^8.13.0",
    "mongodb-memory-server-global": "^6.6.1",
    "react-router-dom": "^6.14.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.1.3"
  }
}
